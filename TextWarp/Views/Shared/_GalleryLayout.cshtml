<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width" />
    <link href="~/lib/bootstrap5/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/css/stylesheet.css" rel="stylesheet">
    <link href="~/lib/fontawesome/css/all.min.css" rel="stylesheet">
    <link href="//code.jquery.com/ui/1.13.2/themes/base/jquery-ui.css" rel="stylesheet">
    <link href="~/css/tagify.css" rel="stylesheet">
    <title>@ViewData["title"]</title>
    @await RenderSectionAsync("Styles", required: false)
</head>
<body style="height: 100vh;position: relative;">
    @RenderBody()
    <div class="d-none align-items-center justify-content-center w-100 h-100" id="loader">
        <div>
            <img src="~/imgs//loading.svg" style="width:80px;height:80px;"/>
        </div>
    </div>
</body>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/js/jQuery.tagify.min.js"></script>
<script src="~/js/jquery.autocomplete.js"></script>
@await RenderSectionAsync("Scripts", required: false)
<script src="~/lib/fontawesome/js/all.min.js"></script>
<script src="~/lib/bootstrap5/js/bootstrap.min.js"></script>
<script src="~/js/GalleryScript.js"></script>
</html>